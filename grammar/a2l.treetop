grammar A2lGrammar
  rule document
    whitespace*
    ( characteristic whitespace* )*
    <A2l::Document>
  end

  rule characteristic
    '/begin CHARACTERISTIC'
    (
      !'/end CHARACTERISTIC'
      (. / newline)
    )*
    '/end CHARACTERISTIC'
    <A2l::Characteristic>
  end

  rule whitespace
    [\s]
  end

  rule newline
    [\n]
  end
end
